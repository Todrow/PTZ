import openpyxl

wb = openpyxl.load_workbook("PyProgs\Выгрузка задачи Битрикс.xlsx")
sheet = wb.active
amount = {}



for i in range(1, length):
    task_name = sheet.cell(column=2, row=i).value
    first_two = task_name[:2]
    if (first_two == 'ПЭ'):
        if (sheet.cell(column=10, row=i).value != 'Завершена'):
                tag = sheet.cell(column=21, row=i).value
                tags = tag.split(', ')
                for each in tags:
                    if (each in amount):
                        amount[each] += 1
                    else:
                        amount[each] = 1
                


print(amount)



